<%-
  require 'csv'

  csv_output = CSV.generate(:col_sep => ",") do |csv|
    columns = SupplejackApi::SiteActivity.activities

    csv << columns
    SupplejackApi::SiteActivity.all.each do |site_activity|
      csv << columns.map {|c| site_activity.send(c) }
    end
  end
%>
<%= csv_output.html_safe %>